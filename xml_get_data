import xml.etree.ElementTree as ET

xmax = None
xmin = None
ymax = None
ymin = None
for i in range(150, 151):
    M3 = f'./DSC_{i:04d}.xml'
    M4 = str(M3)
    tree = ET.parse(M4)
    root = tree.getroot()
    filename = root[1].text
    for o in root.iter('object'):

